  <ThingDef ParentName="BaseMeleeWeapon_SharpGreen_Quality">
    <defName>Teamson_Greenskin_RustedLongspear</defName>
    <label>Rusted Longspear</label>
    <techLevel>Medieval</techLevel>
    <description>A rusted Spear made from a weapon looted on the battlefield. A particularly favorite weapon of the barbarian clansmen of middling wealth. A deadly cutter, and allowing distance to strike at targets with odd percision for such a crude thing.</description>
    <graphicData>
      <texPath>Weapons/Barbarian_2hspear</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.8</drawSize>
    </graphicData>
    <uiIconScale>1.2</uiIconScale>
    <weaponTags>
      <li>Teamson_Greenskins_T1_2h</li>
      <li>Teamson_Greenskins_T0_2h</li>

    </weaponTags>
    <weaponClasses>
      <li>Neolithic</li>
    </weaponClasses>
    <statBases>
      <WorkToMake>1000</WorkToMake>
      <Mass>1</Mass>
    </statBases>
    <equippedAngleOffset>-65</equippedAngleOffset>
    <burnableByRecipe>True</burnableByRecipe>
    <tools>
      <li>
        <label>handle</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>11</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>point</label>
        <capacities>
          <li>Stab</li>
        </capacities>
        <power>24</power>
        <cooldownTime>2.4</cooldownTime>
        <armorPenetration>0.3</armorPenetration>
      </li>
	  <li>
        <label>edge</label>
        <capacities>
          <li>Cut</li>
        </capacities>
        <power>18</power>
        <cooldownTime>2.8</cooldownTime>
        <armorPenetration>0.4</armorPenetration>
      </li>
    </tools>

  </ThingDef>
